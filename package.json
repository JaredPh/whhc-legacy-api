{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "SEE LICENSE IN LICENSE.md",
  "precommit": [
    "precommit"
  ],
  "scripts": {
    "codacy": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "start": "node index.js",
    "start:watch": "nodemon",
    "start:prod": "node index.js",
    "prestart:prod": "tsc",
    "precommit": "npm run test",
    "test": "npm run test:unit && npm run test:snyk",
    "test:unit": "nyc ./node_modules/mocha/bin/mocha --require ts-node/register tests/**/*.spec.ts",
    "test:snyk": "snyk test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/microservices": "^4.6.6",
    "@nestjs/testing": "^4.6.6",
    "@nestjs/typeorm": "^2.0.0",
    "@nestjs/websockets": "^4.6.6",
    "bcrypt": "^1.0.3",
    "cognito-express": "^2.0.10",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "crypto": "^1.0.1",
    "dotenv-safe": "^5.0.1",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^8.2.0",
    "moment": "^2.22.0",
    "mysql": "^2.15.0",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.8",
    "snyk": "^1.80.0",
    "typeorm": "^0.2.6",
    "typescript": "^2.8.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/node": "^9.6.2",
    "@types/sinon": "^4.3.1",
    "chai": "^4.1.2",
    "codacy-coverage": "^2.1.1",
    "dotenv": "^5.0.1",
    "mocha": "^5.0.5",
    "mock-req": "^0.2.0",
    "nodemon": "^1.17.3",
    "nyc": "^11.6.0",
    "pre-commit": "^1.2.2",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1"
  },
  "snyk": true
}
